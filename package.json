{
  "name": "react-pay-icons",
  "version": "1.1.0",
  "description": "Payment Icons for React applications",
  "main": "./lib/index.js",
  "scripts": {
    "dev": "webpack-dev-server --config ./webpack.dev.js --mode development",
    "icons": "svgr --config-file ./svgr.config.js -d ./src/Icons ./IconsSource",
    "build": "npm run icons && rimraf ./lib && BABEL_ENV=es babel ./src/Icons --out-dir ./lib"
  },
  "files": [
    "/lib"
  ],
  "author": "Tommy Ljungberg",
  "license": "MIT",
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "repository": "https://github.com/twltwl/react-pay-icons",
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/plugin-transform-modules-umd": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@svgr/cli": "^6.2.1",
    "babel-loader": "^8.2.2",
    "rimraf": "^3.0.0",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.3"
  }
}
